<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xky.dao.TeacherDao">
    <select id="selectAllTeacher" resultType="Teacher">
           select * from teacher;
    </select>

    <insert id="insertTeacher" useGeneratedKeys="true" keyProperty="teacherId">
          insert into teacher value (null,#{name},#{gender},#{age},#{phone},#{address},#{imgPath});
    </insert>
</mapper>